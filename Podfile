source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

inhibit_all_warnings!

def common_pods
  pod 'Alamofire', '~> 4.8.2'
end

target 'Planter' do
  use_frameworks!

  common_pods
end

target 'PlanterTests' do
  use_frameworks!

  common_pods
end


target 'PlanterUITests' do
  use_frameworks!

  common_pods
end

